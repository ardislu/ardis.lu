    "@ngneat/spectator": "^6.0.0",
    "@schematics/angular": "^10.1.7",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "codelyzer": "^6.0.1",
    "jest": "^26.5.3",
    "jest-preset-angular": "^8.3.1",
    "local-web-server": "^4.2.1",
    "ng-mocks": "^10.2.1",
    "playwright": "^1.5.1",
    "protractor": "^7.0.0",
    "source-map-explorer": "^2.5.0",
