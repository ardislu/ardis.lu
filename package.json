{
  "name": "ardis-lu-frontend",
  "version": "0.0.0",
  "description": "This is the frontend for a personal website.",
  "keywords": [
    "angular",
    "angularjs",
    "personal",
    "frontend"
  ],
  "contributors": [
    "Ardis Lu <ardis@ardis.lu> (https://ardis.lu)"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stage": "ng build --prod && cd dist/ardis-lu-frontend && ws --spa index.html --log.format stats",
    "analyze": "ng build --prod --source-map --vendor-chunk=false && source-map-explorer dist/ardis-lu-frontend/main.*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.5",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "^11.0.5",
    "@angular/compiler": "^11.0.5",
    "@angular/core": "^11.0.5",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.0.5",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "^11.0.5",
    "@angular/platform-browser-dynamic": "^11.0.5",
    "@angular/router": "^11.0.5",
    "@angular/service-worker": "^11.0.5",
    "@auth0/auth0-spa-js": "^1.13.5",
    "fuse.js": "^6.4.4",
    "hammerjs": "^2.0.8",
    "ngx-google-analytics": "^11.0.0",
    "ngx-skeleton-loader": "^2.6.2",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.0.0",
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular-eslint/builder": "0.8.0-beta.7",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.7",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.7",
    "@angular-eslint/schematics": "^0.8.0-beta.7",
    "@angular-eslint/template-parser": "0.8.0-beta.7",
    "@angular/cli": "^11.0.5",
    "@angular/compiler-cli": "^11.0.5",
    "@ngneat/spectator": "^6.1.2",
    "@schematics/angular": "^11.0.5",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.17",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "eslint": "^7.16.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.9",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "local-web-server": "^4.2.1",
    "ng-mocks": "^11.2.8",
    "playwright": "^1.7.1",
    "protractor": "^7.0.0",
    "source-map-explorer": "^2.5.1",
    "ts-node": "~9.1.1"
  }
}
