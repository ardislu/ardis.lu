{
  "name": "ardis-lu-frontend",
  "version": "0.0.0",
  "description": "This is the frontend for a personal website.",
  "keywords": [
    "angular",
    "angularjs",
    "personal",
    "frontend"
  ],
  "contributors": [
    "Ardis Lu <ardis@ardis.lu> (https://ardis.lu)"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stage": "ng build --prod && cd dist/ardis-lu-frontend && ws --spa index.html --log.format stats",
